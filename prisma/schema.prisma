// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Feature {
  id     Int     @id @default(autoincrement())
  name    String?
  exposure Int
  createdAt DateTime @default(now())
}

model User {
  id      Int      @id @default(autoincrement())
  name    String?
  createdAt DateTime @default(now())
}

model Flag {
  id      Int      @id @default(autoincrement())
  user    Int
  feature Int
  createdAt DateTime @default(now())
}